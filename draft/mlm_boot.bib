@article{lai2021,
	title = {Bootstrap confidence intervals for multilevel standardized effect size},
	volume = {56},
	issn = {0027-3171, 1532-7906},
	url = {https://www.tandfonline.com/doi/full/10.1080/00273171.2020.1746902},
	doi = {10.1080/00273171.2020.1746902},
	abstract = {Although many methodologists and professional organizations have urged applied researchers to compute and report eﬀect size measures accompanying tests of statistical signiﬁcance, discussions on obtaining conﬁdence interval (CI) for eﬀect size with clustered/multilevel data have been scarce. In this paper, I explore the bootstrap as a viable and accessible alternative for obtaining CIs for multilevel standardized mean diﬀerence eﬀect size for cluster-randomized trials. A simulation was carried out to compare 17 analytic and bootstrap procedures for constructing CIs for multilevel eﬀect size, in terms of empirical coverage rates and width, for both normal and nonnormal data. Results showed that, overall, the residual bootstrap with studentized CI had the best coverage rates (94.75\% on average), whereas the residual bootstrap with basic CI had better coverage in small samples. These two procedures for constructing CIs showed better coverage than using analytic methods for both normal and nonnormal data. In addition, I provide an illustrative example showing how bootstrap CIs for multilevel eﬀect size can be easily obtained using the statistical software R and the R package bootm m. I strongly encourage applied researchers to report CIs to adequately convey the uncertainty of their eﬀect size estimates.},
	language = {en},
	number = {4},
	urldate = {2022-08-17},
	journal = {Multivariate Behavioral Research},
	author = {Lai, Mark H. C.},
	month = jul,
	year = {2021},
	pages = {558--578},
	file = {Lai - 2021 - Bootstrap Confidence Intervals for Multilevel Stan.pdf:/Users/yichi/Zotero/storage/HJRHGDC4/Lai - 2021 - Bootstrap Confidence Intervals for Multilevel Stan.pdf:application/pdf},
}

