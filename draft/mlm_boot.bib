@phdthesis{busing1993,
	title = {Distribution characteristics of variance estimates in {two-level} models: a {Monte} {Carlo} study},
	shorttitle = {{DISTRIBUTION} {CHARACTERISTICS} {OF} {VARIANCE} {ESTIMATES} {IN} {TWO}-{LEVEL} {MODELS}},
	abstract = {In this paper Monte Carlo methods were used to study the distribution characteristics of variance estimates in two-level models. Little is known, in general, of the small sample properties of these estimates. Monte Carlo simulation has been used to determine the extent of agreement with the large sample theory in small sample situations. Confirmations of theoretical results were found in most areas. The Level-1 variance estimate is quite accurate, but for the smallest total sample size. The skewed distribution (skewed to the right) for the variance estimates was confirmed. Owing to this, the bias of parameter estimates is negative. The bias is quite large in the small sample situation. In some cases the results deviate severely from what one would expect. As a result of the unequal proportion of the bias of the parameter estimate and the bias of the estimate for the corresponding standard error, certain tests based upon these estimates will be conservative. The minimum sample size indicated in the literature will not suffice. Depending on the level, the number of observations should be at least 25. Sample sizes on which the calculation of the estimates depends, are especially important.},
	author = {Busing, Frank},
	month = jan,
	year = {1993},
	school = {Leiden University},
	doi = {10.13140/RG.2.2.18116.94088},
	file = {Full Text PDF:/Users/yichi/Zotero/storage/K9F3L5I7/Busing - 1993 - DISTRIBUTION CHARACTERISTICS OF VARIANCE ESTIMATES.pdf:application/pdf},
}

